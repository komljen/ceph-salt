{
  "handlers" : {
    "default" : {
      "handlers" : ["stdout"],
      "type" : "set"
    },
    "expiry" : {
      "command" : "/etc/sensu/plugins/sensu/delete-expired-stashes.rb",
      "severities" : ["critical"],
      "type" : "pipe"
    },
    "stdout" : {
      "command" : "cat",
      "type" : "pipe"
    }
  }
}
